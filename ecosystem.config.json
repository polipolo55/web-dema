{
  "apps": [{
    "name": "dema-website",
    "script": "server.js",
    "instances": 1,
    "exec_mode": "cluster",
    "watch": false,
    "max_memory_restart": "500M",
    "env": {
      "NODE_ENV": "production",
      "PORT": 3000
    },
    "log_file": "./logs/app.log",
    "error_file": "./logs/error.log",
    "out_file": "./logs/out.log",
    "time": true,
    "max_restarts": 10,
    "min_uptime": "10s"
  }]
}
